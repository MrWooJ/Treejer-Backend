{
  "name": "client",
  "base": "User",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "hidden": [
    "realm",
    "emailVerified"
  ],
  "mixins": {
    "Fetcher": {}
  },
  "properties": {
    "email": {
      "type": "string",
      "required": true,
      "index": true
    },
    "username": {
      "type": "string",
      "required": true,
      "index": true
    },
    "firstname": {
      "type": "string",
      "required": true,
      "index": true
    },
    "lastname": {
      "type": "string",
      "required": true,
      "index": true
    },
    "password": {
      "type": "string",
      "required": true,
      "index": true
    },
    "status": {
      "type": "string",
      "required": true
    },
    "createDate": {
      "type": "number",
      "required": true
    },
    "lastUpdate": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "trees": {
      "type": "hasMany",
      "model": "tree",
      "foreignKey": "clientId"
    },
    "receipts": {
      "type": "hasMany",
      "model": "receipt",
      "foreignKey": "clientId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "login"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "changeStatus"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__receipts"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__trees"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
