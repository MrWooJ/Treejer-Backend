{
  "name": "voucher",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Fetcher": {}
  },
  "properties": {
    "clientId": {
      "type": "string",
      "required": true
    },
    "createDate": {
      "type": "number",
      "required": true
    },
    "lastUpdate": {
      "type": "number",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    },
    "numberOfUsage": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "usageCapacity": {
      "type": "number",
      "required": true
    },
    "items": {
      "type": [
        "object"
      ],
      "required": true
    },
    "receivers": {
      "type": [
        "string"
      ],
      "required": false,
      "default": []
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "findById"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
